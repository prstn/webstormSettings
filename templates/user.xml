<templateSet group="user">
  <template name="log" value="console.log($MODULE_REF$);" description="console.log" toReformat="true" toShortenFQNames="true">
    <variable name="MODULE_REF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ngdoc" value="@ngdoc $TYPE$&#10;* @name $NAME$&#10;* @description&#10;* $DESC$" description="Start a ngdoc block" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESC" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>